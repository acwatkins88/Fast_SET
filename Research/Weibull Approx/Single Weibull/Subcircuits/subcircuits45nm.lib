.lib std_cells

.subckt and2inp a b vdd z
MP1 i a vdd vdd CMOSP W=100n L=45n  
MP2 i b vdd vdd CMOSP W=100n L=45n 
MN1 i a cn1 0 CMOSN W=50n L=45n 
MN2 cn1 b 0 0 CMOSN W=50n L=45n
mp3 z i vdd vdd CMOSP W=100n L=45n
mn3 z i 0 0 CMOSN W=50n L=45n
.ends and2inp

.subckt nand2inp a b vdd z
